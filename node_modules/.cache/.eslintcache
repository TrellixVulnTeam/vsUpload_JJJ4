  // Replay animation by hiding & showing the element again\n  let el = document.body;\n  el.addEventListener(\"click\", function (e) {\n    el.hidden = true;\n    requestAnimationFrame(() => {\n      el.hidden = false;\n    });\n  });\n\n  // work testing\n\n  return (\n    <div className=\"workContainer\" id=\"app\">\n      <div className=\"logoContainerSpacer logoHeadingSpacer\">\n        <ul>\n          <li>\n            <NavLink\n              className=\"workLink\"\n              exact\n              activeClassName=\"activeWorkLink\"\n              to=\"/work/\"\n              id=\"section-item\"\n            >\n              all work\n            </NavLink>\n          </li>\n          <li>\n            <NavLink\n              className=\"workLink\"\n              activeClassName=\"activeWorkLink\"\n              to=\"/work/ux\"\n              id=\"section-item\"\n            >\n              ux\n            </NavLink>\n          </li>\n          <li>\n            <NavLink\n              className=\"workLink\"\n              activeClassName=\"activeWorkLink\"\n              to=\"/work/web\"\n              id=\"section-item\"\n            >\n              web\n            </NavLink>\n          </li>\n          <li>\n            <NavLink\n              className=\"workLink\"\n              activeClassName=\"activeWorkLink\"\n              to=\"/work/graphics\"\n              id=\"section-item\"\n            >\n              graphics\n            </NavLink>\n          </li>\n          <li>\n            <NavLink\n              className=\"workLink\"\n              activeClassName=\"activeWorkLink\"\n              to=\"/work/java\"\n              id=\"section-item\"\n            >\n              java\n            </NavLink>\n          </li>\n          <li>\n            <NavLink\n              className=\"workLink\"\n              activeClassName=\"activeWorkLink\"\n              to=\"/work/photo\"\n              id=\"section-item\"\n            >\n              photo\n            </NavLink>\n          </li>\n        </ul>\n      </div>\n\n      <Switch>\n        // section switch\n        {routes.map((route, i) => (\n          <RouteWithSubRoutes key={i} {...route} />\n        ))}\n        // all work list - change to featured later\n        <div>\n          <SectionMenu menuItems={projects} />\n          {/* <a href=\"#\" class=\"image-hover\">\n            <span class=\"title style2\" data-title=\"G\">\n              G\n            </span>\n            <img src=\"/img/doodle/G.jpeg\" alt=\"\" />\n          </a> */}\n        </div>\n      </Switch>\n    </div>\n  );\n}\n\nfunction GraphicsMenu() {\n  return (\n    <div className=\"sectionMenuContainer\">\n      <SectionMenu\n        menuItems={projects\n          .filter((project) => project.topics.includes(\"graphics\"))\n          .map((filteredProject) => filteredProject)}\n      />\n    </div>\n  );\n}\n\nfunction JavaMenu() {\n  return (\n    <div className=\"sectionMenuContainer\">\n      <SectionMenu\n        menuItems={projects\n          .filter((project) => project.topics.includes(\"java\"))\n          .map((filteredProject) => filteredProject)}\n      />\n    </div>\n  );\n}\n\nfunction PhotoMenu() {\n  return (\n    <div className=\"sectionMenuContainer\">\n      <SectionMenu\n        menuItems={projects\n          .filter((project) => project.topics.includes(\"photo\"))\n          .map((filteredProject) => filteredProject)}\n      />\n    </div>\n  );\n}\n\nfunction UxMenu() {\n  return (\n    <div className=\"sectionMenuContainer\">\n      <SectionMenu\n        menuItems={projects\n          .filter((project) => project.topics.includes(\"ux\"))\n          .map((filteredProject) => filteredProject)}\n      />\n    </div>\n  );\n}\n\nfunction WebMenu() {\n  return (\n    <div className=\"sectionMenuContainer\">\n      <SectionMenu\n        menuItems={projects\n          .filter((project) => project.topics.includes(\"web\"))\n          .map((filteredProject) => f